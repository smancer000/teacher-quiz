{
    "name": "teacher-quiz",
    "version": "0.1.0",
    "description": "A simple quiz platform for teachers",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "start": "node ./dist/index.js",
        "dev": "nodemon --watch src --ext ts,js --exec \"ts-node src/index.ts\"",
        "build": "tsc -p tsconfig.json",
        "test": "jest",
        "lint": "eslint \"src/**/*.{ts,js}\" --fix",
        "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/your-org/teacher-quiz.git"
    },
    "keywords": [
        "quiz",
        "education",
        "teacher"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/your-org/teacher-quiz/issues"
    },
    "homepage": "https://github.com/your-org/teacher-quiz#readme",
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1"
    },
    "devDependencies": {
        "typescript": "^5.5.0",
        "ts-node": "^10.9.1",
        "@types/node": "^20.5.1",
        "@types/express": "^4.17.19",
        "nodemon": "^2.0.22",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "@types/jest": "^29.5.3",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.3.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.9.0",
        "husky": "^8.0.3"
    }
}